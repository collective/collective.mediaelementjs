<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="collective.mediaelement">

  <records prefix="plone.resources/mediaelementjs"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
     <value key="js">++resource++collective.mediaelementjs/mediaelement-and-player.js</value>
  </records>
  <records prefix="plone.resources/mediaelementjs-init"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
     <value key="js">++resource++collective.mediaelement-init.js</value>
     <value key="css">
     </value>
  </records>

  <records prefix="plone.bundles/mediaelementjs-bundle"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>mediaelementjs</element>
      <element>mediaelementjs-init</element>
    </value>
    <value key="stub_js_modules">
      <element>jquery</element>
    </value>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++collective.mediaelement-bundle.js</value>
    <value key="csscompilation">++resource++collective.mediaelementjs/mediaelementplayer.css</value>
    <!-- <value key="last_compilation">2015-05-28 00:00:00</value> -->
  </records>

</registry>
